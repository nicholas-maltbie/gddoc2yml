{
  "build": {
    "dest": "_site",
    "template": [
      "default",
      "modern",
      "godot-template"
    ],
    "globalMetadata": {
      "_appTitle": "Example Project Docs",
      "_appFooter": "Example Project Docs",
      "_enableSearch": true
    },
    "content": [
      {
        "files": ["*.yml"],
        "src": "api",
        "dest": "api"
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "xref": [
      "https://gddoc2yml.nickmaltbie.com/xrefmap/godot_xrefmap.yml"
    ]
  }
}